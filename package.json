  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "esbuild": "^0.17.11",
    "sass": "^1.58.3"
  },
  "devDependencies": {
    "@rails/webpacker": "5.4.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3"
  },
  "resolutions": {
    "openssl": "^2.0.0"
  }
}

